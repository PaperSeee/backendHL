{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/update",
      "dest": "src/app.js"
    },
    {
      "src": "/(.)",
      "dest": "src/app.js"
    }
  ],
  "cron": [
    {
      "path": "/api/update",
      "schedule": "/1 * * * *"
    }
  ]
}